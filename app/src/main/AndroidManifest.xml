<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.vcu.team100_minigolfapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Team100_miniGolfApp">
        <activity android:name=".EditCourse"></activity>
        <activity android:name=".Courses" />
        <activity android:name=".Settings" />
        <activity android:name=".Results" />
        <activity android:name=".ScoreCard" />
        <activity android:name=".CreateNewGame" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>